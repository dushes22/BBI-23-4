using System;
class Program
{
    static void Main()
    {
        double x = -4.0;
        double step = 0.5;
        Console.WriteLine("x\t| y");
        while (x <= 4)
        {
            double y = 0.5 * Math.Pow(x, 2) - 7 * x;
            Console.WriteLine($"{x}\t| {y}");
            x += step;
        }
    }
}
