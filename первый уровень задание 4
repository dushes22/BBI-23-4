using System;
class Program
{
    static void Main()
    {
        double x = Math.PI / 4; 
        double sum = 0;
        for (int i = 1; i <= 9; i++)
        {
            double term = Math.Cos(i * x) / Math.Pow(x, i - 1);
            sum += term;
        }
        Console.WriteLine("Сумма: " + sum);
    }
}
