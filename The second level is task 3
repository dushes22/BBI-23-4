using System;

class Program
{
    static void Main()
    {
        int a = 3; 
        int h = 2; 
        int p = 100; 
        int sum = 0; 
        int n = 0; 
        while (sum + a <= p)
        {
            sum += a;
            a += h;
            n++;
        }
        Console.WriteLine("Количество членов арифметической прогрессии: " + n);
    }
}
