using System;
class Program
{
    static void Main()
    {
        int n = 1;
        int product = 1;
        int L = 30000;
        while (product * (3 * n + 1) <= L)
        {
            product *= 3 * n + 1;
            n++;
        }
        Console.WriteLine("Наибольшее значение сомножителя n: " + (n - 1));
    }
}
